[notice]
info=这是 .conf 文件的示例；跑通部分测试文件，可能需要在同目录下配置 .conf 文件！
about_consul_and_nacos=使用远程配置服务时，优先从 consul 中获取配置信息
detail=以下为所需的配置信息，具体值请自行添加。

[mysql]
host=***
port=3306
user=***
password=***
database=***
charset=*** # 可选参数，默认 utf8mb4

[mongodb]
host=***
port=27017
database=***
authsource=***
user=***
password=***
# 也可用 uri 的方式设置连接配置，会优先从 uri 中获取，示例如下:
uri=mongodb://user:password@host:port/database?authSource=*&authMechanism=* # 两种方式请按需选择一种即可

[postgresql]
host=***
port=5432
user=***
password=***
database=***
charset=*** # 可选参数，默认 UTF8

[mq]
host=***
port=5672
username=***
password=***
virtualhost=***
queue=***
exchange=***
routing_key=***

[kafka]
bootstrap_servers=host:port #若多个用逗号分隔
topic=***
key=***

[consul]
token=
url=http://host:port/v1/kv/...?dc=dc1&raw
format=json

[nacos]
url=http://host:port/nacos/v1/cs/configs?dataId=***&group=***&tenant=***
format=json

[kdl_dynamic_proxy]
proxy=o668.kdltps.com:15818
username=***
password=***

[kdl_exclusive_proxy]
proxy=http://kps.kdlapi.com/api/getkps?orderid=***&num=100&format=json
username=***
password=***
index=1

[ali_oss]
accesskeyid=LTA***
accesskeysecret=***
endpoint=https://oss-cn-***.aliyuncs.com
bucket=***
doc=***
